SHELL = /bin/bash

CPPFLAGS = -I./eigen-2.0
MAKE = make -s

default: doxygen test

doxygen:
	@echo "  DOXYGEN Doxyfile"
	@doxygen Doxyfile > doxygen.log

test:
	@echo "  TEST"
	@(cd tests && $(MAKE) test)

clean:
	@echo "  CLEAN docs"
	@rm -rf docs include/confdefs.h
	@echo "  CLEAN tests"
	@(cd tests && $(MAKE) clean)

